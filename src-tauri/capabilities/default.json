{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "fs:default",
    {
      "identifier": "fs:allow-video-read",
      "allow": [{ "path": "**" }]
    },
    "core:default",
    "dialog:default"
  ]
}
